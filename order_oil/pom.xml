        <artifactId>team_oil</artifactId>
        <version>1.0-SNAPSHOT</version>
    </parent>
    <groupId>com.ouyu.tech.team_oil</groupId>
    <artifactId>order_oil</artifactId>
    <version>1.0-SNAPSHOT</version>
    <name>order_oil</name>
    <description>团油的工单服务</description>
    <properties>
